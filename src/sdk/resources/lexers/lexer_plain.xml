<?xml version="1.0"?>
<!DOCTYPE CodeBlocks_lexer_properties>
<CodeBlocks_lexer_properties>
        <!-- The filemasks attribute is empty, because we don't want this file to match anything, we
            handle this specially.
        -->
        <Lexer name="Plain text files"
                index="1"
                filemasks="">
                <Style name="Default"
                        index="0,11"
                        fg="0,0,0"
                        bg="255,255,255"
                        bold="0"
                        italics="0"
                        underlined="0"/>
                <Style name="Default (inactive)"
                        index="64,72,75"
                        fg="200,200,200"/>
                <Style name="Selection"
                        index="-99"
                        bg="192,192,192"/>
                <Style name="Active line"
                        index="-98"
                        bg="255,255,160"/>
                <Style name="Breakpoint line"
                        index="-2"
                        bg="255,160,160"/>
                <Style name="Matching brace highlight"
                        index="34"
                        fg="0,0,0"
                        bg="128,255,255"
                        bold="1"/>
                <Style name="No matching brace highlight"
                        index="35"
                        fg="255,255,255"
                        bg="255,0,0"
                        bold="1"/>
                <SampleCode value="lexer_cpp.sample"/>
        </Lexer>
</CodeBlocks_lexer_properties>
